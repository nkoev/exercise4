<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="home.kpmg.exercise4.controller.Details"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page titleAlignment="Center" title="Event Details">
		<List>
			<InputListItem label="Event">
				<Input value="{EventName}"/>
			</InputListItem>
			<InputListItem label="Location">
				<Input value="{City}"/>
			</InputListItem>
		</List>
	</Page>
</mvc:View>